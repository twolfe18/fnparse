CP=`find target -iname '*.jar' | tr '\n' ':'`
echo "classpath: ${CP}"
echo "args: $@"

/export/common/tools/jdk1.8.0_25/bin/java -Xmx6G -ea -cp ${CP} \
  -XX:+UseG1GC -XX:G1ReservePercent=2 -XX:ConcGCThreads=2 -XX:ParallelGCThreads=2 \
  edu.jhu.hlt.fnparse.inference.frameid.BasicFeatureTemplates $@

